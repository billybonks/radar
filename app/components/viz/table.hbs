<table>
  <thead>
    {{#each this.columns as |column|}}
      <th>{{column.name}}</th>
    {{/each}}
  </thead>
  <tbody>
    {{#each @results as |row| }}
      <tr>
        {{#each this.columns as |column|}}
          <td>{{get row column.valuePath}}</td>
        {{/each}}
      </tr>
    {{/each}}
  </tbody>
</table>